<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="590.0" prefWidth="751.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PopravkeMajstorPage">
    <children>
        <TableView fx:id="repairmentsTable" layoutX="31.0" layoutY="41.0" prefHeight="421.0" prefWidth="704.0">
            <columns>
            <TableColumn fx:id="repairCol" prefWidth="75.0" text="Id popravke" />
            <TableColumn fx:id="reportCol" prefWidth="90.0" text="Id prijave" />
                <TableColumn fx:id="descriptionCol" prefWidth="350.0" text="Opis" />
                <TableColumn fx:id="startDateCol" minWidth="0.0" prefWidth="238.0" text="Početak" />
            </columns>
        </TableView>
        <Button fx:id="endBtn" layoutX="542.0" layoutY="481.0" mnemonicParsing="false" onAction="#endRepairment" prefHeight="51.0" prefWidth="121.0" text="Završi popravku" />
      <Button fx:id="serviceBtn" layoutX="103.0" layoutY="481.0" mnemonicParsing="false" onAction="#addService" prefHeight="51.0" prefWidth="133.0" text="Evidentiraj korištenje usluge" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="partBtn" layoutX="324.0" layoutY="481.0" mnemonicParsing="false" onAction="#addBackUpPart" prefHeight="51.0" prefWidth="133.0" text="Evidentiraj korištenje rez. dijela" textAlignment="CENTER" wrapText="true" />
    </children>
</AnchorPane>
